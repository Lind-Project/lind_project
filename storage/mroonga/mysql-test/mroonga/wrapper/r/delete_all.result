DROP TABLE IF EXISTS users;
SET NAMES utf8;
CREATE TABLE users (
id int PRIMARY KEY,
name varchar(100),
FULLTEXT INDEX (name)
) COMMENT 'engine = "InnoDB"' DEFAULT CHARSET=utf8;
