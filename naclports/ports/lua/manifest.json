{
  "name": "Lua",
  "description": "Lua interactive shell built using Native Client",
  "version": "5.3.0.%(version)s",
  "manifest_version": 2,
  "minimum_chrome_version": "30",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "unlimitedStorage"
  ]
}
