DROP TABLE IF EXISTS items;
CREATE TABLE items (
name VARCHAR(255),
price TINYINT UNSIGNED KEY
) DEFAULT CHARSET=utf8;
INSERT INTO items VALUES ("hamburger", 255);
INSERT INTO items VALUES ("discount", 0);
INSERT INTO items VALUES ("coke", 100);
SELECT * FROM items;
name	price
