DROP TABLE IF EXISTS temperatures;
CREATE TABLE temperatures (
id INT PRIMARY KEY AUTO_INCREMENT,
title VARCHAR(20),
temperature DOUBLE,
KEY temperature_index(temperature),
KEY multi_index(temperature, title)
);
