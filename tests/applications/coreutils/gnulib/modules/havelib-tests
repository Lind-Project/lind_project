  havelib/rpathz/configure \
  havelib/rpathlx/Makefile.in \
  havelib/rpathlx/aclocal.m4 \
  havelib/rpathlx/configure \
  havelib/rpathly/Makefile.in \
  havelib/rpathly/aclocal.m4 \
  havelib/rpathly/configure \
