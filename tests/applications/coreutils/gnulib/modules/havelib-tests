  havelib/Makefile.in \
  havelib/rpathx/Makefile.in \
  havelib/rpathx/aclocal.m4 \
  havelib/rpathx/configure \
  havelib/rpathy/Makefile.in \
  havelib/rpathy/aclocal.m4 \
  havelib/rpathy/configure \
