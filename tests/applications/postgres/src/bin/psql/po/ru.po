msgstr "  \\z      [МАСКА]        то же, что и \\dp\n"

#: help.c:267
#, c-format
msgid "Formatting\n"
msgstr "Форматирование\n"

#: help.c:268
#, c-format
msgid ""
"  \\a                     toggle between unaligned and aligned output mode\n"
msgstr ""
"  \\a                     переключение режимов вывода:\n"
