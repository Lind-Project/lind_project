msgstr "  \\ds[S+] [МАСКА]        список последовательностей\n"

#: help.c:255
#, c-format
msgid "  \\dt[S+] [PATTERN]      list tables\n"
msgstr "  \\dt[S+] [МАСКА]        список таблиц\n"

