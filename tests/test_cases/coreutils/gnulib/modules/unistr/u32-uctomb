gl_LIBUNISTRING_MODULE([0.9], [unistr/u32-uctomb])

Makefile.am:
if LIBUNISTRING_COMPILE_UNISTR_U32_UCTOMB
lib_SOURCES += unistr/u32-uctomb.c
endif

