@hex:
  # Bad example of using lea solution (instead of add) to update rbp.
  # That is where we use rsp in address instead of rbp.
  #    mov %ebp, %ecx
  #    lea %rbp, [%rsp+%r15*1]
  89 cd
  4a 8d 2c 3c
